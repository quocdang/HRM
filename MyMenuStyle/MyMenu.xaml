<UserControl x:Class="HRM.MyMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HRM"
             mc:Ignorable="d" 
             >
    <Menu x:Name="menu">
        <MenuItem x:Name="File" Header="File"/>
        <MenuItem x:Name="Edit" Header="Edit"/>
        <MenuItem x:Name="View" Header="Edit"/>
        <MenuItem x:Name="Go" Header="Go"/>
        <MenuItem x:Name="Tool" Header="Tool"/>

    </Menu>
</UserControl>
